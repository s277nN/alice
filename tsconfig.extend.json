{
  "compilerOptions": {
    "baseUrl": "./src",
    "isolatedModules": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@styles/*": [
        "src/assets/styles/*"
      ],
      "@image/*": [
        "src/assets/images/*"
      ]
    }
  }
}